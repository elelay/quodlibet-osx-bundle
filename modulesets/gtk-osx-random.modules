	    version="0.9.3">
      <patch file="http://git.gnome.org/browse/gtk-osx/plain/patches/libunistring-clang.patch" strip="1"/>
    </branch>
  </autotools>

  <autotools id="bdw-gc" autogen-sh="configure">
